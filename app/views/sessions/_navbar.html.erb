<%= link_to 'Home', root_url %>
<% if logged_in? %>
  <%= link_to 'Sign out', logout_path, method: :delete %>
  <%= link_to 'My Profile', user_path(User.find(session[:id])) %>
  <%= link_to 'Post an Image', new_user_image_path(User.find(session[:id])) %>
<% else %>
  <%= link_to 'Sign In', login_path %>
  <%= link_to 'Register', new_user_path %>
<% end %>